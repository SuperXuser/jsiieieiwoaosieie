-- Get token with cool animation
local function getToken(userId)
    StatusLabel:UpdateLabel("ğŸ”„ Requesting access token...")
    wait(0.5)
    
    local success, response = pcall(function()
        return request({
            Url = ApiUrl .. "/get-token",
            Method = "POST",
            Headers = {
                ["Content-Type"] = "application/json",
                ["User-Agent"] = getExecutor() .. " Free/2.0"
            },
            Body = game:GetService("HttpService"):JSONEncode({
                robloxUserId = userId
            })
        })
    end)
    
    if success and response and response.StatusCode == 200 then
        StatusLabel:UpdateLabel("âœ… Token acquired!")
        local data = game:GetService("HttpService"):JSONDecode(response.Body)
        return data.token
    end
    
    StatusLabel:UpdateLabel("âŒ Failed to get token!")
    return nil
end

-- Check free trial status with cool messages
local function checkFreeStatus()
    TrialLabel:UpdateLabel("ğŸ Checking free trial status...")
    wait(0.5)
    
    local success, response = pcall(function()
        return request({
            Url = ApiUrl .. "/admin/free-status?key=1182291Xx@@",
            Method = "GET",
            Timeout = 5
        })
    end)
    
    if success and response and response.StatusCode == 200 then
        local data = game:GetService("HttpService"):JSONDecode(response.Body)
        return data.freeVersionEnabled
    end
    return nil
end

