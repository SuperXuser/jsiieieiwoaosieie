-- ================= UTILITY FUNCTIONS =================

-- Generate unique nonce
local function generateNonce()
    return tostring(os.time()) .. "_" .. math.random(100000, 999999)
end

-- Get executor name with cool formatting
local function getExecutor()
    local executors = {
        {"Delta", "ğŸŸ "},
        {"Synapse", "ğŸ”´"},
        {"Krnl", "ğŸŸ£"},
        {"Fluxus", "ğŸŸ¢"},
        {"Scriptware", "ğŸ”µ"},
        {"Electron", "âšª"},
        {"Celery", "ğŸŸ¡"},
        {"Xeno", "âš«"},
        {"Solara", "ğŸŸ¤"},
        {"Arceus", "ğŸ”µ"},
        {"Hydrogen", "âšª"},
        {"Sentinel", "ğŸ”´"}
    }
    
    local success, result = pcall(function()
        return identifyexecutor()
    end)
    
    if success and result then
        local execName = result:lower()
        for _, exe in ipairs(executors) do
            if execName:find(exe[1]:lower()) then
                return exe[1], exe[2]
            end
        end
    end
    return "Unknown", "âš«"
end

