-- Load free script with style
local function loadFreeScript()
    StatusLabel:UpdateLabel("ğŸ”¥ PREPARING TO LOAD HOODHELLER ğŸ”¥")
    wait(0.5)
    
    local player = game:GetService("Players").LocalPlayer
    local userId = player.UserId
    local executor, executorColor = getExecutor()
    
    ExecutorLabel:UpdateLabel(executorColor .. " Executor: " .. executor)
    
    -- Show loading animation
    showLoadingAnimation()
    
    -- Get token
    local token = getToken(userId)
    if not token then
        StatusLabel:UpdateLabel("âŒ Authentication failed!")
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "HoodHeller",
            Text = "Failed to authenticate! Check Discord.",
            Duration = 3,
            Button1 = "OK"
        })
        return
    end
    
    StatusLabel:UpdateLabel("ğŸ“¦ Loading script package...")
    wait(0.5)
    
    -- Prepare request
    local requestData = {
        robloxUserId = userId,
        nonce = generateNonce(),
        executor = executor,
        placeId = game.PlaceId,
        timestamp = os.time() * 1000
    }
    
    -- Load script
    local success, response = pcall(function()
        return request({
            Url = ApiUrl .. "/load?isFree=true",
            Method = "POST",
            Headers = {
                ["Content-Type"] = "application/json",
                ["Authorization"] = "Bearer " .. token,
                ["User-Agent"] = executor .. " Free/2.0"
            },
            Body = game:GetService("HttpService"):JSONEncode(requestData)
        })
    end)
    
    if success and response and response.StatusCode == 200 then
        StatusLabel:UpdateLabel("ğŸš€ EXECUTING SCRIPT...")
        wait(0.5)
        
        local loadSuccess, loadResult = pcall(function()
            loadstring(response.Body)()
        end)
        
        if loadSuccess then
            StatusLabel:UpdateLabel("âœ… HOODHELLER LOADED SUCCESSFULLY!")
            
            -- Party time! ğŸ‰
            for i = 1, 3 do
                game:GetService("StarterGui"):SetCore("SendNotification", {
                    Title = "HoodHeller",
                    Text = "ğŸš€ Free trial activated!",
                    Duration = 1
                })
                wait(0.2)
            end
            
            -- Update status indicators
            for _, child in ipairs(StatusSection:GetChildren()) do
                if child:IsA("TextLabel") and child.Text:find("API Connection") then
                    child:UpdateLabel("ğŸŸ¢ API Connection: Connected")
                elseif child:IsA("TextLabel") and child.Text:find("Free Trial") then
                    child:UpdateLabel("ğŸŸ£ Free Trial: Active")
                end
            end
        else
            StatusLabel:UpdateLabel("âŒ Execution failed!")
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = "HoodHeller",
                Text = "Failed to execute script!",
                Duration = 3,
                Button1 = "OK"
            })
        end
    else
        StatusLabel:UpdateLabel("âŒ Failed to load script!")
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "HoodHeller",
            Text = "Failed to load script!",
            Duration = 3,
            Button1 = "OK"
        })
    end
end

